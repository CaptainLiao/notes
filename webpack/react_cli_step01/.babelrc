{
  "presets": [
    ["latest", {"es2015": {"loose": true}}],
    "react",
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "bable-runtime"
    }]
  ]
}